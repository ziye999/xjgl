		<action name="examstumodifyNo_*" class="com.jiajie.action.registration.ExamStuModifyNoAction" method="{1}">
		</action>
	</package>
	<package name="review" namespace="/" extends="struts-default,json-default">
		<action name="review_*" class="com.jiajie.action.registration.ExamReviewAction" method="{1}">
		</action>
		<action name="reviewXs_*" class="com.jiajie.action.registration.ExamReviewXsAction" method="{1}">
		</action>
		<action name="reviewBb_*" class="com.jiajie.action.registration.ExamReviewBbAction" method="{1}">
