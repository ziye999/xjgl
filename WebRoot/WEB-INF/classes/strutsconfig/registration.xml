	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<package name="registration" namespace="/" extends="struts-default,json-default">
		<action name="registration_*" class="com.jiajie.action.registration.ExamRegistrationAction" method="{1}">
